{
  "name": "tgs",
  "version": "1.0.3-0",
  "description": "Troublor General Purpose Server",
  "repository": "https://github.com/Troublor/tgs",
  "author": "William Aaron Cheung",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --fix .",
    "test": "NODE_OPTIONS=--experimental-vm-modules yarn jest --runInBand",
    "typeorm": "typeorm-ts-node-esm -d src/database/data-source.migration.ts",
    "dev": "ts-node --esm src/index.ts",
    "build": "tsc",
    "start": "yarn node dist/index.js",
    "release": "zsh ./scripts/release.zsh",
    "docker:build": "docker build -t troublor/tgs:$(jq -r .version package.json) .",
    "docker:push": "docker push troublor/tgs:$(jq -r .version package.json)"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@nestjs/common": "^9.0.11",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.11",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/typeorm": "^9.0.1",
    "@octokit/rest": "^19.0.5",
    "@troubkit/log": "^0.4.0",
    "async": "^3.2.4",
    "axios": "^0.27.2",
    "class-validator": "^0.13.2",
    "isomorphic-fetch": "^3.0.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "nest-winston": "^1.7.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6",
    "simple-git": "^3.15.1",
    "telegraf": "^4.9.2",
    "typeorm": "^0.3.10",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.11",
    "@types/async": "^3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/js-yaml": "^4",
    "@types/lodash": "^4",
    "@types/node": "^18.7.14",
    "@types/pg": "^8",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "8.22.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "jest-serial-runner": "^1.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-jest-resolver": "^2.0.0",
    "ts-node": "^10.9.1",
    "type-fest": "^3.3.0",
    "typescript": "^4.8.2"
  },
  "stableVersion": "1.0.2"
}
